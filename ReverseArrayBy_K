class Solution(object):


    def reverse(self,nums,i,j):
        li = i
        ri = j
        while li < ri:
            temp=nums[li]
            nums[li]=nums[ri]
            nums[ri]=temp

            li+=1
            ri-=1


    def rotate(self, nums: list[int], k: int)
        """
        :type nums: List[int]
        :type k: int
        :rtype: None Do not return anything, modify nums in-place instead.
        """
        k= k% lens(num)
        if k<0 :
           k +=len(nums)
        self.reverse(nums,0,lens(nums)-k-1);